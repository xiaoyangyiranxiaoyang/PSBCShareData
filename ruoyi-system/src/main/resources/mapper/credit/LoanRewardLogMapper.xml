<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.psbc.credit.mapper.LoanRewardLogMapper">

    <resultMap type="com.psbc.credit.domain.LoanRewardLog" id="LoanRewardLogMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="loadRctId" column="load_rct_id" jdbcType="VARCHAR"/>
        <result property="num" column="num" jdbcType="INTEGER"/>
        <result property="createBy" column="create_by" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="serverIp" column="server_ip" jdbcType="VARCHAR"/>
        <result property="updateBy" column="update_by" jdbcType="VARCHAR"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="delFlag" column="del_flag" jdbcType="INTEGER"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="LoanRewardLogMap">
        select
          id, load_rct_id, num, create_by, create_time, server_ip, update_by, update_time, del_flag
        from ruoyi.loan_reward_log
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="LoanRewardLogMap">
        select
          id, load_rct_id, num, create_by, create_time, server_ip, update_by, update_time, del_flag
        from ruoyi.loan_reward_log
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="LoanRewardLogMap">
        select
        id, load_rct_id, num, create_by, create_time, server_ip, update_by, update_time, del_flag
        from ruoyi.loan_reward_log
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="loadRctId != null and loadRctId != ''">
                and load_rct_id = #{loadRctId}
            </if>
            <if test="num != null">
                and num = #{num}
            </if>
            <if test="createBy != null and createBy != ''">
                and create_by = #{createBy}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="serverIp != null and serverIp != ''">
                and server_ip = #{serverIp}
            </if>
            <if test="updateBy != null and updateBy != ''">
                and update_by = #{updateBy}
            </if>
            <if test="updateTime != null">
                and update_time = #{updateTime}
            </if>
            <if test="delFlag != null">
                and del_flag = #{delFlag}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into ruoyi.loan_reward_log(load_rct_id, num, create_by, create_time, server_ip, update_by, update_time, del_flag)
        values (#{loadRctId}, #{num}, #{createBy}, #{createTime}, #{serverIp}, #{updateBy}, #{updateTime}, #{delFlag})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update ruoyi.loan_reward_log
        <set>
            <if test="loadRctId != null and loadRctId != ''">
                load_rct_id = #{loadRctId},
            </if>
            <if test="num != null">
                num = #{num},
            </if>
            <if test="createBy != null and createBy != ''">
                create_by = #{createBy},
            </if>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="serverIp != null and serverIp != ''">
                server_ip = #{serverIp},
            </if>
            <if test="updateBy != null and updateBy != ''">
                update_by = #{updateBy},
            </if>
            <if test="updateTime != null">
                update_time = #{updateTime},
            </if>
            <if test="delFlag != null">
                del_flag = #{delFlag},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from ruoyi.loan_reward_log where id = #{id}
    </delete>

</mapper>